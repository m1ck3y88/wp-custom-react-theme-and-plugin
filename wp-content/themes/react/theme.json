{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultPalette": false,
      "palette": [
        { "slug": "r-white", "color": "rgb(255,255,255)", "name": "React White" },
        { "slug": "r-gray-100", "color": "rgb(243 244 246)", "name": "React Gray 100" },
        { "slug": "r-gray-200", "color": "rgb(229 231 235)", "name": "React Gray 200" },
        { "slug": "r-gray-300", "color": "rgb(209 213 219)", "name": "React Gray 300" },
        { "slug": "r-gray-400", "color": "rgb(156 163 175)", "name": "React Gray 400" },
        { "slug": "r-gray-500", "color": "rgb(107 114 128)", "name": "React Gray 500" },
        { "slug": "r-gray-600", "color": "rgb(75 85 99)", "name": "React Gray 600" },
        { "slug": "r-gray-700", "color": "rgb(55 65 81)", "name": "React Gray 700" },
        { "slug": "r-gray-800", "color": "rgb(31 41 55)", "name": "React Gray 800" },
        { "slug": "r-gray-900", "color": "rgb(17 24 39)", "name": "React Gray 900" },
        { "slug": "r-primary", "color": "rgb(239 68 68)", "name": "React Primary" },
        { "slug": "r-accent", "color": "rgb(67 56 202)", "name": "React Accent" },
        { "slug": "r-success", "color": "rgb(52 211 153)", "name": "React Success" },
        { "slug": "r-info", "color": "rgb(96 165 250)", "name": "React Info" },
        { "slug": "r-warning", "color": "rgb(251 146 60)", "name": "React Warning" },
        { "slug": "r-danger", "color": "rgb(236 72 153)", "name": "React Danger" }
      ],
      "background": true,
      "text": true,
      "link": true,
      "custom": true,
      "duotone": [
        { "slug": "r-pink-sunset", "colors": ["#11245E", "#DC4379"], "name": "React Pink Sunset" }
      ],
      "defaultGradients": false,
      "gradients": [
        { "slug": "r-summer-dog", "name": "React Summer Dog", "gradient": "linear-gradient(#A8FF78, #78FFD6)" }
      ]
    },
    "blocks": {
      "core/site-title": {
        "color": {
          "defaultPalette": true,
          "palette": [
            { "slug": "r-primary", "color": "rgb(239 68 68)", "name": "React Primary" }
          ],
          "link": true,
          "custom": false,
          "gradients": [],
          "customGradient": false
        },
        "typography": {
          "fontFamilies": [
            {
              "fontFamily": "Pacifico, sans-serif",
              "slug": "r-pacifico",
              "name": "React Pacifico"
            }
          ]
        }
      },
      "core/cover": {
        "color": {
          "duotone": [],
          "customDuotone": false
        }
      },
      "core/preformatted": {
        "typography": {
          "fontSizes": [],
          "customFontSize": false
        }
      }
    },
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "Rubik, sans-serif",
          "slug": "r-rubik",
          "name": "React Rubik"
        }
      ],
      "fontSizes": [
        {
          "slug": "small",
          "size": "0.75rem",
          "name": "Small"
        },
        {
          "slug": "medium",
          "size": "1.25rem",
          "name": "Medium"
        },
        {
          "slug": "large",
          "size": "2.25rem",
          "name": "Large"
        },
        {
          "slug": "x-large",
          "size": "3rem",
          "name": "Extra Large"
        },
        {
          "slug": "gigantic",
          "size": "3.75rem",
          "name": "Gigantic"
        }
      ],
      "lineHeight": true,
      "dropCap": true,
      "fontWeight": true,
      "fontStyle": true,
      "textTransform": true,
      "letterSpacing": true,
      "textDecoration": true
    },
    "layout": {
      "contentSize": "800px",
      "wideSize": "1100px"
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "units": [
        "px", "rem", "vh", "vw", "%"
      ],
      "blockGap": true
    }
  },
  "styles": {
    "color": {
      "text": "var(--wp--preset--color--r-gray-700)",
      "background": "var(--wp--preset--color--r-white)"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color--r-primary)"
        }
      }
    },
    "blocks": {
      "core/button": {
        "color": {
          "gradient": "var(--wp--preset--gradient--r-summer-dog)"
        }
      },
      "core/pullquote": {
        "border": {
          "width": "2px",
          "radius": "10px",
          "style": "solid",
          "color": "var(--wp--preset--color--u-primary)"
        }
      },
      "core/site-title": {
        "typography": {
          "fontFamily": "var(--wp--reset--font-family--r-pacifico)"
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--r-rubik)",
      "fontSize": "16px",
      "fontStyle": "normal",
      "fontWeight": "normal",
      "lineHeight": "inherit",
      "textDecoration": "none",
      "textTransform": "none"
    },
    "spacing": {
      "margin": {
        "top": "0px",
        "bottom": "0px",
        "left": "0px",
        "right": "0px"
      },
      "padding": {
        "top": "0px",
        "bottom": "0px",
        "left": "0px",
        "right": "0px"
      },
      "blockGap": "4rem"
    }
  }
}